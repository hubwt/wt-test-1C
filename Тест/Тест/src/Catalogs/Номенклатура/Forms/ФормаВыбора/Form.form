    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиентаТовары.Ссылка) КАК Ссылка,
	ЗаказКлиентаТовары.Номенклатура КАК Номенклатура,
	ЗаказКлиентаТовары.Ссылка.Состояние КАК Состояние
ПОМЕСТИТЬ ВТ_Заявки
ИЗ
	Документ.ЗаказКлиента.Товары КАК ЗаказКлиентаТовары
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПродажаЗапчастей КАК ПродажаЗапчастей
		ПО ЗаказКлиентаТовары.Ссылка = ПродажаЗапчастей.ЗаказКлиента
ГДЕ
	ЗаказКлиентаТовары.Ссылка.Состояние = ЗНАЧЕНИЕ(Перечисление.дт_СостоянияЗаказовКлиента.ЖдёмДенег)
	И ПродажаЗапчастей.ЗаказКлиента ЕСТЬ NULL
СГРУППИРОВАТЬ ПО
	ЗаказКлиентаТовары.Номенклатура,
	ЗаказКлиентаТовары.Ссылка.Состояние
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ
	СправочникНоменклатура.Ссылка КАК Ссылка,
	СправочникНоменклатура.Код КАК Код,
	СправочникНоменклатура.Наименование КАК Наименование,
	СправочникНоменклатура.Артикул КАК Артикул,
	СправочникНоменклатура.НомерПроизводителя КАК НомерПроизводителя,
	СправочникНоменклатура.Бренд КАК Бренд,
	МестаХранения.МестоХранения КАК МестоХранения,
	РегистрНакопления1Остатки.КолвоОстаток КАК КолвоОстаток,
	СправочникНоменклатура.НомерЗаменИндекс КАК НомерЗаменИндекс,
	СправочникНоменклатура.РекомендованаяЦена КАК РекомендованаяЦена,
	СправочникНоменклатура.Категория КАК Категория,
	ВТ_Заявки.Состояние КАК Состояние,
	СправочникНоменклатура.Состояние КАК СостояниеТовара
{ГДЕ
	(СправочникНоменклатура.Категория = &amp;Категория)}
ИЗ
	Справочник.Номенклатура КАК СправочникНоменклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.РегистрНакопления1.Остатки(, Склад = &amp;Склад) КАК РегистрНакопления1Остатки
		ПО СправочникНоменклатура.Ссылка = РегистрНакопления1Остатки.Товар
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.дт_МестаХраненияНоменклатуры.СрезПоследних(, Склад = &amp;Склад) КАК МестаХранения
		ПО СправочникНоменклатура.Ссылка = МестаХранения.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Заявки КАК ВТ_Заявки
		ПО СправочникНоменклатура.Ссылка = ВТ_Заявки.Номенклатура</queryText>
      <mainTable>Catalog.Номенклатура</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
