    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	СУММА(Расходы.Сумма) КАК Сумма,
	Расходы.ЗаявкаНаРасход КАК ЗаявкаНаРасход
ПОМЕСТИТЬ Расходы
ИЗ
	Документ.Расходы КАК Расходы

СГРУППИРОВАТЬ ПО
	Расходы.ЗаявкаНаРасход
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ЗаявкаРасход.СуммаДокумента - ЕСТЬNULL(Расходы.Сумма, 0)) КАК ОстатокПоЗаявкам,
	ЗаявкаРасход.Счет КАК Счет
ПОМЕСТИТЬ ВТ_ОстатокПоЗаявкам
ИЗ
	Документ.ЗаявкаНаРасход КАК ЗаявкаРасход
		ЛЕВОЕ СОЕДИНЕНИЕ Расходы КАК Расходы
		ПО (Расходы.ЗаявкаНаРасход = ЗаявкаРасход.Ссылка)
ГДЕ
	ЗаявкаРасход.ДатаОплаты МЕЖДУ ДАТАВРЕМЯ(2024,01,01) И КОНЕЦПЕРИОДА(&amp;Период, ДЕНЬ)
	И ЗаявкаРасход.СуммаДокумента > ЕСТЬNULL(Расходы.Сумма, 0)
	И ЗаявкаРасход.ПометкаУдаления = ЛОЖЬ
	И ЗаявкаРасход.Статус &lt;> ЗНАЧЕНИЕ(Перечисление.дт_СтатусыЗаявокНаРасход.Отклонено) 
	
СГРУППИРОВАТЬ ПО
	ЗаявкаРасход.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(Док.СуммаДокумента - ЕСТЬNULL(Расходы.Сумма, 0)) КАК СуммаДокумента,
	Док.Счет.Код КАК СчетКод
ПОМЕСТИТЬ ВТ_Планируемыерасходы
ИЗ
	Документ.ЗаявкаНаРасход КАК Док
		ЛЕВОЕ СОЕДИНЕНИЕ Расходы КАК Расходы
		ПО (Расходы.ЗаявкаНаРасход = Док.Ссылка)
ГДЕ
	Док.ДатаОплаты МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Период, ДЕНЬ) И КОНЕЦПЕРИОДА(&amp;Период, ДЕНЬ)
	И Док.СуммаДокумента > ЕСТЬNULL(Расходы.Сумма, 0)
	И Док.ПлнируемыйРасход = ИСТИНА
	И Док.ПометкаУдаления = ЛОЖЬ
	И Док.Статус &lt;> ЗНАЧЕНИЕ(Перечисление.дт_СтатусыЗаявокНаРасход.Отклонено)  
СГРУППИРОВАТЬ ПО
	Док.Счет.Код
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ПриходДенегНаСчет.СуммаДокумента) КАК СуммаДокумента,
	ПриходДенегНаСчет.Счет.Код КАК СчетКод
ПОМЕСТИТЬ ВТ_Приход_Денег
ИЗ
	Документ.ПриходДенегНаСчет КАК ПриходДенегНаСчет
ГДЕ
	ПриходДенегНаСчет.Дата МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Период, ДЕНЬ) И КОНЕЦПЕРИОДА(&amp;Период, ДЕНЬ)
	И ПриходДенегНаСчет.Проведен = ИСТИНА
	И ПриходДенегНаСчет.ПометкаУдаления = ЛОЖЬ

СГРУППИРОВАТЬ ПО
	ПриходДенегНаСчет.Счет.Код
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РегистрНакопленияБалансОстатки.Измерение1 КАК Счет,
	ВЫБОР
		КОГДА РегистрНакопленияБалансОстатки.БалансОстаток ЕСТЬ NULL
			ТОГДА &quot;0&quot;
		ИНАЧЕ РегистрНакопленияБалансОстатки.БалансОстаток
	КОНЕЦ КАК БалансОстаток,
	ВЫБОР
		КОГДА ВТ_ОстатокПоЗаявкам.ОстатокПоЗаявкам ЕСТЬ NULL
			ТОГДА &quot;0&quot;
		ИНАЧЕ ВТ_ОстатокПоЗаявкам.ОстатокПоЗаявкам
	КОНЕЦ КАК ОстатокПоЗаявка,
	ВЫБОР
		КОГДА ВТ_Планируемыерасходы.СуммаДокумента ЕСТЬ NULL
			ТОГДА &quot;0&quot;
		ИНАЧЕ ВТ_Планируемыерасходы.СуммаДокумента
	КОНЕЦ КАК ПланируемыеЗаявкиНаРасход,
	ВЫБОР
		КОГДА ВТ_Приход_Денег.СуммаДокумента ЕСТЬ NULL
			ТОГДА &quot;0&quot;
		ИНАЧЕ ВТ_Приход_Денег.СуммаДокумента
	КОНЕЦ КАК ПриходДенегНаСчета
ИЗ
	РегистрНакопления.Баланс.Остатки(НАЧАЛОПЕРИОДА(&amp;Период, ДЕНЬ), ) КАК РегистрНакопленияБалансОстатки
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Планируемыерасходы КАК ВТ_Планируемыерасходы
		ПО РегистрНакопленияБалансОстатки.Измерение1.Код = ВТ_Планируемыерасходы.СчетКод
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Приход_Денег КАК ВТ_Приход_Денег
		ПО РегистрНакопленияБалансОстатки.Измерение1.Код = ВТ_Приход_Денег.СчетКод
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ОстатокПоЗаявкам КАК ВТ_ОстатокПоЗаявкам
		ПО РегистрНакопленияБалансОстатки.Измерение1 = ВТ_ОстатокПоЗаявкам.Счет
ГДЕ
	РегистрНакопленияБалансОстатки.Измерение1 В (&amp;МассивСчетов)</queryText>
      <mainTable>AccumulationRegister.Баланс.Balance</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
