  <attributes>
    <name>СписокСчета</name>
    <title>
      <key>ru</key>
      <value>Список счета</value>
    </title>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	СУММА(Расходы.Сумма) КАК Сумма,
	Расходы.ЗаявкаНаРасход КАК ЗаявкаНаРасход
ПОМЕСТИТЬ Расходы
ИЗ
	Документ.Расходы КАК Расходы

СГРУППИРОВАТЬ ПО
	Расходы.ЗаявкаНаРасход
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ЗаявкаРасход.СуммаДокумента - ЕСТЬNULL(Расходы.Сумма, 0)) КАК ОстатокПоЗаявкам,
	ЗаявкаРасход.Счет КАК Счет
ПОМЕСТИТЬ ВТ_ОстатокПоЗаявкам
ИЗ
	Документ.ЗаявкаНаРасход КАК ЗаявкаРасход
		ЛЕВОЕ СОЕДИНЕНИЕ Расходы КАК Расходы
		ПО (Расходы.ЗаявкаНаРасход = ЗаявкаРасход.Ссылка)
ГДЕ
	(ЗаявкаРасход.Дата МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Период, ДЕНЬ) И КОНЕЦПЕРИОДА(&amp;Период, ДЕНЬ)
				И ЗаявкаРасход.СуммаДокумента > ЕСТЬNULL(Расходы.Сумма, 0)
				
				И ЗаявкаРасход.ПланируемыйРасход = ЛОЖЬ)
	И ЗаявкаРасход.ПометкаУдаления = ЛОЖЬ

СГРУППИРОВАТЬ ПО
	ЗаявкаРасход.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(Док.СуммаДокумента - ЕСТЬNULL(Расходы.Сумма, 0)) КАК СуммаДокумента,
	Док.Счет.Код КАК СчетКод
ПОМЕСТИТЬ ВТ_Планируемыерасходы
ИЗ
