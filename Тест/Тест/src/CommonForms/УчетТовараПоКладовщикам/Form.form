      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ИндНомер.Ответственный КАК Ответственный,
	КОЛИЧЕСТВО(ИндНомер.индкод) КАК КоличествоДень,
	СУММА(ИндНомер.Цена) КАК СуммаДень
ПОМЕСТИТЬ ВТ_УчтенныйТоварДень
ИЗ
	РегистрСведений.ИндНомер КАК ИндНомер
ГДЕ
	ИндНомер.ДатаИзмененияКонвеера МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Дата, ДЕНЬ) И КОНЕЦПЕРИОДА(&amp;Дата, ДЕНЬ)

СГРУППИРОВАТЬ ПО
	ИндНомер.Ответственный
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ИндНомер.Ответственный КАК Ответственный,
	КОЛИЧЕСТВО(ИндНомер.индкод) КАК КоличествоНеделя,
	СУММА(ИндНомер.Цена) КАК СуммаНеделя
ПОМЕСТИТЬ ВТ_УчтенныйТоварНеделя
ИЗ
	РегистрСведений.ИндНомер КАК ИндНомер
ГДЕ
	ИндНомер.ДатаИзмененияКонвеера МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Дата, НЕДЕЛЯ) И КОНЕЦПЕРИОДА(&amp;Дата, НЕДЕЛЯ)

СГРУППИРОВАТЬ ПО
	ИндНомер.Ответственный
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ИндНомер.Ответственный КАК Ответственный,
	КОЛИЧЕСТВО(ИндНомер.индкод) КАК КоличествоМесяц,
	СУММА(ИндНомер.Цена) КАК СуммаМесяц
ПОМЕСТИТЬ ВТ_УчтенныйТоварМесяц
ИЗ
	РегистрСведений.ИндНомер КАК ИндНомер
ГДЕ
	ИндНомер.ДатаИзмененияКонвеера МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Дата, МЕСЯЦ) И КОНЕЦПЕРИОДА(&amp;Дата, МЕСЯЦ)

СГРУППИРОВАТЬ ПО
	ИндНомер.Ответственный
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ИндНомер.Ответственный КАК Ответственный,
	КОЛИЧЕСТВО(ИндНомер.индкод) КАК КоличествоКвартал,
	СУММА(ИндНомер.Цена) КАК СуммаКвартал
ПОМЕСТИТЬ ВТ_УчтенныйТоварКвартал
ИЗ
	РегистрСведений.ИндНомер КАК ИндНомер
ГДЕ
	ИндНомер.ДатаИзмененияКонвеера МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Дата, КВАРТАЛ) И КОНЕЦПЕРИОДА(&amp;Дата, КВАРТАЛ)

СГРУППИРОВАТЬ ПО
	ИндНомер.Ответственный
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Пользователи.Ссылка КАК Учетчик,
	ВТ_УчтенныйТоварДень.КоличествоДень КАК КоличествоДень,
	ВТ_УчтенныйТоварДень.СуммаДень КАК СуммаДень,
	ВТ_УчтенныйТоварНеделя.КоличествоНеделя КАК КоличествоНеделя,
	ВТ_УчтенныйТоварНеделя.СуммаНеделя КАК СуммаНеделя,
	ВТ_УчтенныйТоварМесяц.КоличествоМесяц КАК КоличествоМесяц,
	ВТ_УчтенныйТоварМесяц.СуммаМесяц КАК СуммаМесяц,
	ВТ_УчтенныйТоварКвартал.КоличествоКвартал КАК КоличествоКвартал,
	ВТ_УчтенныйТоварКвартал.СуммаКвартал КАК СуммаКвартал
ИЗ
	Справочник.Пользователи КАК Пользователи
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_УчтенныйТоварДень КАК ВТ_УчтенныйТоварДень
		ПО (ВТ_УчтенныйТоварДень.Ответственный = Пользователи.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_УчтенныйТоварНеделя КАК ВТ_УчтенныйТоварНеделя
		ПО (ВТ_УчтенныйТоварНеделя.Ответственный = Пользователи.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_УчтенныйТоварМесяц КАК ВТ_УчтенныйТоварМесяц
		ПО (ВТ_УчтенныйТоварМесяц.Ответственный = Пользователи.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_УчтенныйТоварКвартал КАК ВТ_УчтенныйТоварКвартал
		ПО (ВТ_УчтенныйТоварКвартал.Ответственный = Пользователи.Ссылка)
ГДЕ
	(ВТ_УчтенныйТоварДень.КоличествоДень &lt;> 0
			ИЛИ ВТ_УчтенныйТоварНеделя.КоличествоНеделя &lt;> 0
			ИЛИ ВТ_УчтенныйТоварМесяц.КоличествоМесяц &lt;> 0
			ИЛИ ВТ_УчтенныйТоварКвартал.КоличествоКвартал &lt;> 0)</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
