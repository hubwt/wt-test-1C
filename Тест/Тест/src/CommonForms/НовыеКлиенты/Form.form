    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ЗаказКлиента.Клиент КАК Клиент,
	КОЛИЧЕСТВО(ЗаказКлиента.Ссылка) КАК КоличествоЗаказов
ПОМЕСТИТЬ КоличествоЗаявок
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
СГРУППИРОВАТЬ ПО
	ЗаказКлиента.Клиент

ИНДЕКСИРОВАТЬ ПО
	Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПродажаЗапчастей.Клиент КАК Клиент,
	КОЛИЧЕСТВО(ПродажаЗапчастей.Ссылка) КАК КоличествоПродаж,
	СУММА(ПродажаЗапчастей.ИтогоРекв) КАК СуммаПродаж
ПОМЕСТИТЬ КоличествоИСуммаПродаж
ИЗ
	Документ.ПродажаЗапчастей КАК ПродажаЗапчастей
СГРУППИРОВАТЬ ПО
	ПродажаЗапчастей.Клиент

ИНДЕКСИРОВАТЬ ПО
	Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МАКСИМУМ(ЗаказКлиента.Дата) КАК ДатаПоследнегоОбращения,
	ЗаказКлиента.Клиент КАК Клиент
ПОМЕСТИТЬ ВТ_ПослОбр
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
СГРУППИРОВАТЬ ПО
	ЗаказКлиента.Клиент

ИНДЕКСИРОВАТЬ ПО
	Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Клиенты.Код,
	Клиенты.Ссылка КАК Клиент,
	Клиенты.Телефон,
	Клиенты.ДатаСоздания,
	Клиенты.Город2 КАК Город,
	Клиенты.ТипКлиента,
	Клиенты.ВидКлиента,
	Клиенты.КоличествоАвтомобилей КАК КоличествоМашин,
	Клиенты.Ответственный КАК Менеджер,
	ВЫБОР
		КОГДА Клиенты.Ответственный = Значение(Справочник.Пользователи.ПустаяСсылка)
		ИЛИ Клиенты.Ответственный.Подразделение.Город = Значение(Справочник.Город.ПустаяСсылка)
		ИЛИ Клиенты.Ответственный.Подразделение = Значение(Справочник.Подразделения.ПустаяСсылка)
		ИЛИ Клиенты.Ответственный = &amp;ОбщийКлиент
			ТОГДА &quot;Общие клиенты&quot;
		ИНАЧЕ Клиенты.Ответственный.Подразделение.Город
	КОНЕЦ КАК Филиал,
	КоличествоЗаявок.КоличествоЗаказов КАК КоличествоЗаявок,
	КоличествоИСуммаПродаж.КоличествоПродаж,
	КоличествоИСуммаПродаж.СуммаПродаж,
	ВТ_ПослОбр.ДатаПоследнегоОбращения
ИЗ
	Справочник.Клиенты КАК Клиенты
		ЛЕВОЕ СОЕДИНЕНИЕ КоличествоЗаявок КАК КоличествоЗаявок
		ПО КоличествоЗаявок.Клиент = Клиенты.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПослОбр КАК ВТ_ПослОбр
		ПО Клиенты.Ссылка = ВТ_ПослОбр.Клиент
		ЛЕВОЕ СОЕДИНЕНИЕ КоличествоИСуммаПродаж КАК КоличествоИСуммаПродаж
		ПО Клиенты.Ссылка = КоличествоИСуммаПродаж.Клиент
ГДЕ
	Клиенты.ДатаСоздания МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания</queryText>
