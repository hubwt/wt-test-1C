	Расходы.ЗаявкаНаРасход КАК ЗаявкаНаРасход,
	СУММА(Расходы.Сумма) КАК Сумма
ПОМЕСТИТЬ втОплатыПоЗаявкам
ИЗ
	Документ.Расходы КАК Расходы
ГДЕ
	Расходы.Проведен
	И Расходы.ЗаявкаНаРасход &lt;> ЗНАЧЕНИЕ(Документ.ЗаявкаНаРасход.ПустаяСсылка)

СГРУППИРОВАТЬ ПО
	Расходы.ЗаявкаНаРасход
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Док.Ссылка КАК Ссылка,
	Док.ПометкаУдаления КАК ПометкаУдаления,
	Док.Номер КАК Номер,
	Док.Дата КАК Дата,
	Док.Проведен КАК Проведен,
	Док.Комментарий КАК Комментарий,
	Док.Статус КАК Статус,
	Док.Ответственный КАК Ответственный,
	Док.СуммаДокумента КАК СуммаДокумента,
	Док.Согласовал КАК Согласовал,
	Док.Подразделение КАК Подразделение,
	Док.ВидРасхода КАК ВидРасхода,
	Док.ДатаОплаты КАК ДатаОплаты,
	Док.Организация КАК Организация,
	Док.Получатель КАК Получатель,
	Док.Инициатор КАК Инициатор,
	Док.МоментВремени КАК МоментВремени,
	ВЫБОР
		КОГДА втОплатыПоЗаявкам.Сумма > 0
			ТОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0)
		ИНАЧЕ ЕСТЬNULL(ВТ_ПеремещениеПоЗаявкам.Сумма, 0)
	КОНЕЦ КАК СуммаОплачено,
	ВЫБОР
		КОГДА Док.Статус = ЗНАЧЕНИЕ(Перечисление.дт_СтатусыЗаявокНаРасход.Отклонено)
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0) > 0
						ТОГДА 5
					ИНАЧЕ 6
				КОНЕЦ
		КОГДА Док.Согласовал = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0) > 0
						ТОГДА 3
					ИНАЧЕ 4
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0) >= Док.СуммаДокумента
						ИЛИ ЕСТЬNULL(ВТ_ПеремещениеПоЗаявкам.Сумма, 0) >= Док.СуммаДокумента
					ТОГДА 0
				КОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0) > 0
					ТОГДА 1
				ИНАЧЕ 2
			КОНЕЦ
	КОНЕЦ КАК Оплачено,
	ВЫБОР
		КОГДА ЕСТЬNULL(втОплатыПоЗаявкам.Сумма, 0) >= Док.СуммаДокумента
				ИЛИ ЕСТЬNULL(ВТ_ПеремещениеПоЗаявкам.Сумма, 0) >= Док.СуммаДокумента
			ТОГДА &quot;Оплачено&quot;
		ИНАЧЕ &quot;Не оплачено&quot;
	КОНЕЦ КАК СостояниеОплаты,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы,
	Док.Счет КАК Счет,
	Док.ПринятоБанком КАК ПринятоБанком
ИЗ
	Документ.ЗаявкаНаРасход КАК Док
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПеремещениеПоЗаявкам КАК ВТ_ПеремещениеПоЗаявкам
		ПО (ВТ_ПеремещениеПоЗаявкам.ЗаявкаНаРасход = Док.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ втОплатыПоЗаявкам КАК втОплатыПоЗаявкам
		ПО (втОплатыПоЗаявкам.ЗаявкаНаРасход = Док.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО Док.Ссылка = НаличиеФайлов.ОбъектСФайлами
ГДЕ
	Док.Контроль </queryText>
