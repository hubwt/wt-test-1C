			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:use>false</dcscor:use>
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="v8:LocalStringType">
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>ДФ=dd.MM.yyyy</v8:content>
						</v8:item>
					</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СостояниеНеделя</dataPath>
			<field>СостояниеНеделя</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Заявки</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПредставлениеПериодаНеделя</dataPath>
			<field>ПредставлениеПериодаНеделя</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Представление периода неделя</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:use>false</dcscor:use>
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="v8:LocalStringType">
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>ДФ=dd.MM.yyyy</v8:content>
						</v8:item>
					</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиента.Ссылка) КАК Ссылка,
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ) КАК Дата
ПОМЕСТИТЬ ВТ_Продажа
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Продажа)

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиента.Ссылка) КАК Ссылка,
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ) КАК Поле1
ПОМЕСТИТЬ ВТ_Заявки
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ЗаказКлиента.Целевая

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиента.Ссылка) КАК КоличествоНеделя,
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ) КАК ПериодНеделя,
	"Входящие заявки" КАК СостояниеНеделя,
	СТРОКА(ДЕНЬ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ))) + "." + ВЫБОР
		КОГДА МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)) &lt; 10
			ТОГДА "0" + СТРОКА(МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
		ИНАЧЕ СТРОКА(МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
	КОНЕЦ + "." + ПОДСТРОКА(СТРОКА(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)), 7, 4) + " - " + СТРОКА(ДЕНЬ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ))) + "." + ВЫБОР
		КОГДА МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)) &lt; 10
			ТОГДА "0" + СТРОКА(МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
		ИНАЧЕ СТРОКА(МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
	КОНЕЦ + "." + ПОДСТРОКА(СТРОКА(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)), 7, 4) КАК ПредставлениеПериодаНеделя
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ),
	СТРОКА(ДЕНЬ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ))) + "." + ВЫБОР
		КОГДА МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)) &lt; 10
			ТОГДА "0" + СТРОКА(МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
		ИНАЧЕ СТРОКА(МЕСЯЦ(НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
	КОНЕЦ + "." + ПОДСТРОКА(СТРОКА(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)), 7, 4) + " - " + СТРОКА(ДЕНЬ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ))) + "." + ВЫБОР
		КОГДА МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)) &lt; 10
			ТОГДА "0" + СТРОКА(МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
		ИНАЧЕ СТРОКА(МЕСЯЦ(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)))
	КОНЕЦ + "." + ПОДСТРОКА(СТРОКА(КОНЕЦПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ)), 7, 4)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиента.Ссылка),
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ),
	ВЫБОР
		КОГДА ЗаказКлиента.Целевая = ИСТИНА
			ТОГДА "Целевые заявки"
		КОГДА ЗаказКлиента.Целевая = ЛОЖЬ
			ТОГДА "Нецелевые заявки"
	КОНЕЦ,
	NULL
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ),
	ВЫБОР
		КОГДА ЗаказКлиента.Целевая = ИСТИНА
			ТОГДА "Целевые заявки"
		КОГДА ЗаказКлиента.Целевая = ЛОЖЬ
			ТОГДА "Нецелевые заявки"
	КОНЕЦ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЗаказКлиента.Ссылка),
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ),
	ВЫБОР
		КОГДА ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Продажа)
			ТОГДА "Продажа"
		КОГДА ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Отказ)
			ТОГДА "Отказ"
	КОНЕЦ,
	NULL
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И (ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Продажа)
			ИЛИ ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Отказ))

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ЗаказКлиента.Дата, НЕДЕЛЯ),
	ВЫБОР
		КОГДА ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Продажа)
			ТОГДА "Продажа"
		КОГДА ЗаказКлиента.Состояние = ЗНАЧЕНИЕ(перечисление.дт_СостоянияЗаказовКлиента.Отказ)
			ТОГДА "Отказ"
	КОНЕЦ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_Продажа.Ссылка / ВТ_Заявки.Ссылка * 100,
	НАЧАЛОПЕРИОДА(ВТ_Продажа.Дата, НЕДЕЛЯ),
	"Конверсия",
	NULL
ИЗ
	ВТ_Заявки КАК ВТ_Заявки
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Продажа КАК ВТ_Продажа
		ПО ВТ_Заявки.Поле1 = ВТ_Продажа.Дата</query>
	</dataSet>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанныхМесяц</name>
		<field xsi:type="DataSetFieldField">
