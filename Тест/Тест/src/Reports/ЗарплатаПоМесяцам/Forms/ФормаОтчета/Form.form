    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ПродажаЗапчастей.Ссылка КАК Ссылка,
	ЗНАЧЕНИЕ(Перечисление.дт_РолиВПродаже.Продавец) КАК Роль,
	ПродажаЗапчастей.ИтогоРекв КАК СуммаДокумента,
	ПродажаЗапчастей.Оплачено КАК Оплачено
ПОМЕСТИТЬ ВТ_Документы
ИЗ
	Документ.ПродажаЗапчастей КАК ПродажаЗапчастей
ГДЕ
	ПродажаЗапчастей.КтоПродал = &amp;КтоПродал
	И ПродажаЗапчастей.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ПродажаЗапчастейОтветственные.Ссылка,
	ПродажаЗапчастейОтветственные.Роль,
	ПродажаЗапчастейОтветственные.Ссылка.ИтогоРекв,
	ПродажаЗапчастейОтветственные.Ссылка.Оплачено
ИЗ
	Документ.ПродажаЗапчастей.Ответственные КАК ПродажаЗапчастейОтветственные
ГДЕ
	ПродажаЗапчастейОтветственные.Сотрудник = &amp;КтоПродал
	И ПродажаЗапчастейОтветственные.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ПродажаЗапчастейОтветственные.Роль = ЗНАЧЕНИЕ(Перечисление.дт_РолиВПродаже.Продавец)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЗаказНарядОтветственные.Ссылка,
	ЗаказНарядОтветственные.Роль,
	ЗаказНарядОтветственные.Ссылка.СуммаТовары,
	ЗаказНарядОтветственные.Ссылка.СостояниеРасчетов = ЗНАЧЕНИЕ(Перечисление.СостоянияВзаиморасчетов.Оплачено)
ИЗ
	Документ.ЗаказНаряд.Ответственные КАК ЗаказНарядОтветственные
ГДЕ
	ЗаказНарядОтветственные.Сотрудник = &amp;КтоПродал
	И ЗаказНарядОтветственные.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ЗаказНарядОтветственные.Роль = ЗНАЧЕНИЕ(Перечисление.дт_РолиВПродаже.Продавец)
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ
	ВТ_Оплаты.Ссылка КАК Ссылка,
	ВТ_Оплаты.Роль КАК Роль,
	СУММА(ВТ_Оплаты.СуммаДокумента) КАК СуммаДокумента,
	МАКСИМУМ(ВТ_Оплаты.Оплачено) КАК Оплачено
ПОМЕСТИТЬ ВТ_ДокументыГр
ИЗ
	ВТ_Документы КАК ВТ_Оплаты
СГРУППИРОВАТЬ ПО
	ВТ_Оплаты.Ссылка,
	ВТ_Оплаты.Роль
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ
	ПродажаЗапчастей.Ссылка КАК Ссылка,
	ПродажаЗапчастей.СуммаДокумента КАК СуммаДокумента,
	ПродажаЗапчастей.Оплачено КАК Оплачено,
	ПродажаЗапчастей.СуммаДокумента * 0.03 КАК Заработано,
	ПродажаЗапчастей.Ссылка.Комментарий КАК Комментарий,
	ЕСТЬNULL(Оплаты.Сумма, 0) КАК СуммаОплачено,
